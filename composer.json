{
  "name": "gmbit/staff",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension for listing and displaying team members with detailed views.",
  "version": "1.0.0",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^13.0"
  },
  "autoload": {
    "psr-4": {
      "Gmbit\\Staff\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gmbit\\Staff\\Tests\\": "Tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "typo3/cms": {
      "extension-key": "staff"
    }
  }
}